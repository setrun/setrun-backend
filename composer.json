{
    "name": "setrun/setrun-backend",
    "description": "Backend component for SETRUN CMS",
    "keywords": ["yii2", "setrun", "framework", "module", "component", "yii2-extension", "extension"],
    "homepage": "http://setrun.ru",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name" : "Alexey Bashkov",
            "email": "ab@smpage.ru",
            "role" : "Ui Ux Designer"
        },
        {
            "name" : "Denis Utkin",
            "email": "dizirator@gmail.com",
            "role" : "Developer",
            "homepage": "http://github.com/dizirator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "setrun/setrun-sys": "*",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-field-range": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "yiidoc/yii2-redactor": "^2.0",
        "yiidoc/yii2-timeago": "^2.0",
        "kotchuprik/yii2-sortable-widgets": "^2.0",
        "2amigos/yii2-date-picker-widget": "^1.0",
        "2amigos/yii2-transliterator-helper": "^0.1.0",
        "codemix/yii2-localeurls": "^1.4",
        "mongosoft/yii2-upload-behavior": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "name": "yiidoc/yii2-timeago",
            "type": "vcs",
            "url": "git@github.com:dizirator/yii2-timeago.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "setrun\\backend\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
